<h1>Create your gossip</h1>
    <%= form_tag url_for(action: 'create'), method: "post" do %>

      <%= label_tag 'Title' %>
      <%= text_field_tag 'title', @gossip.title %><br>
      <%= label_tag "Content"%>
      <%= text_field_tag "content", @gossip.content%><br><br>
      <%= submit_tag "Create Gossip" %>

    <% end %>
